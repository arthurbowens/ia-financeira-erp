{
  "version": 2,
  "builds": [
    {
      "src": "dist/ia-financeira-erp/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/ia-financeira-erp/$1"
    },
    {
      "src": "/",
      "dest": "/dist/ia-financeira-erp/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
